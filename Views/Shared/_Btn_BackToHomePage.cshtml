@model ScreeningTesterWebSite.Models.ModelBase

@{
    <p>
        @Html.ActionLink(
                linkText: "Back To Screening Tester Main Page",
                actionName: "BackToHomePage",
                controllerName: "Home",
                routeValues: new
                {
                    coID = Model.CoID,
                    loID = Model.LoID,
                    envCode = Model.EnvironCode,
                    languageid = Model.LanguageID,
